{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Orders"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Address": {
      "type": "string",
      "required": true
    },
    "City": {
      "type": "string",
      "required": true
    },
    "Postal_Code": {
      "type": "string",
      "required": true
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "IsCheckout": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "order_details": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-detail.order-detail",
      "mappedBy": "order"
    }
  }
}
